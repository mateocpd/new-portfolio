{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "react-app"],
  "overrides": [
    {
        "files": ["*/.js"],
        "rules": {
            "react-hooks/exhaustive-deps": "off"
        }
    }
],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react-hooks/exhaustive-deps" : "off",
    "react/prop-types": "off"
  }
}
